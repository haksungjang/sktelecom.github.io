        &lt;/td&gt;
        &lt;td&gt;Stable&lt;/td&gt;
        &lt;td&gt;1.0.0&lt;/td&gt;
      &lt;/tr&gt;      
      &lt;tr&gt;
        &lt;td&gt;&lt;a href=&#34;/docs/components/training/chainer/&#34;&gt;Chainer operator&lt;/a&gt;
